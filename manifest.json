{

    "manifest_version": 2,
    "name": "Bad News",
    "version": "1.3.0",
  
    "description": "Helps control digital addiction.",
  
    "icons": {
      "48": "icons/words-48.png",
      "96": "icons/words-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.facebook.com/"],
        "js": ["facebook.js"]
      }
      ,
      {
        "matches": ["*://*.instagram.com/*"],
        "js": ["instagram.js"]
      }
      ,
      {
        "matches": ["*://*.linkedin.com/*"],
        "js": ["linkedin.js"]
      }
      ,
      {
        "matches": ["*://*.yahoo.com/*"],
        "js": ["yahoo.js"]
      }
      ,
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["youtube.js"]
      }
      
    ]
  
  }
  